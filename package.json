{
  "name": "qubomc-api",
  "version": "1.2.0",
  "description": "Unified API for QuboMc Minecraft server, QuboDc Discord bot, and Qubo mod.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc",
    "build:prod": "rimraf dist && tsc --project tsconfig.prod.json",
    "start": "node dist/index.js",
    "start:js": "node index.js",
    "dev": "ts-node-dev --respawn --transpile-only index.ts",
    "dev:debug": "ts-node-dev --inspect --respawn --transpile-only index.ts",
    "prestart": "npm run build",
    "lint": "eslint . --ext .ts,.js",
    "test": "jest",
    "bundle-analyzer": "npm run build && npx bundle-analyzer dist/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/QuboMc/QuboMc-API.git"
  },
  "author": "ItzST4R (QuboMc)",
  "license": "Proprietary",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "discord.js": "^14.0.0",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "helmet": "^7.2.0",
    "node-fetch": "^3.3.2",
    "rcon-client": "^4.0.5",
    "winston": "^3.8.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.19.9",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "rimraf": "^5.0.10",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  }
}
